{
  "name": "turtle",
  "version": "1.0.0",
  "author": "Expo <support@expo.io>",
  "homepage": "https://expo.io",
  "scripts": {
    "start": "EXPENV_OVERRIDES=local-common,local expenv gulp",
    "start:ios": "PLATFORM=ios yarn start",
    "start:android": "PLATFORM=android yarn start",
    "watch": "EXPENV_OVERRIDES=local-common,local expenv gulp watch:source",
    "bin:turtle": "./bin/turtle.js",
    "lint": "eslint src",
    "test": "jest --config jest/unit-config.json --watch",
    "build": "gulp build:server",
    "build:production": "./scripts/build.sh",
    "build:android-tarball:local": "./scripts/android/buildAndroidTarballLocally.sh",
    "init-workingdir:local": "./scripts/initLocalWorkingdir.sh",
    "init-workingdir:remote": "yarn init-workingdir:remote-android && init-workingdir:remote-ios",
    "init-workingdir:remote-android": "./scripts/android/fetchRemoteAndroidTarball.sh",
    "init-workingdir:remote-ios": "./scripts/ios/fetchRemoteIosTarball.sh",
    "docker:pull": "retry5 gcloud docker -- pull gcr.io/exponentjs/turtle-android:latest",
    "docker:build": "yarn build:production android && gcloud docker -- build -t gcr.io/exponentjs/turtle-android:latest -t gcr.io/exponentjs/turtle-android:${TAG} .",
    "docker:push": "retry5 gcloud docker -- push gcr.io/exponentjs/turtle-android:${TAG} && retry5 gcloud docker -- push gcr.io/exponentjs/turtle-android:latest",
    "deploy:android": "./scripts/android/deploy.sh",
    "secrets:init-private": "./scripts/initPrivateSecrets.sh",
    "ssh:android:pods-names": "./scripts/ssh/findAndroidPods.sh",
    "ssh:android:connect": "./scripts/ssh/connectToAndroidPod.sh",
    "ssh:ios:ips": "./scripts/ssh/findIosIps.sh"
  },
  "bin": "./bin/turtle.js",
  "devDependencies": {
    "@types/fs-extra": "^5.0.4",
    "@types/joi": "^13.4.3",
    "@types/lodash": "^4.14.109",
    "@types/raven": "^2.5.1",
    "@types/sharp": "^0.17.8",
    "@types/uuid": "^3.4.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.19.1",
    "eslint-config-universe": "^1.0.7",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-babel": "^7.0.1",
    "gulp-changed": "^3.2.0",
    "gulp-nodemon": "^2.2.1",
    "gulp-plumber": "^1.2.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-typescript": "^4.0.2",
    "jest": "^22.4.3",
    "nodemon": "^1.17.3",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.3",
    "tslint": "^5.10.0",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "async-retry": "^1.2.1",
    "aws-sdk": "^2.226.1",
    "bunyan": "^1.8.12",
    "bunyan-debug-stream": "^1.0.8",
    "bunyan-loggly": "^1.3.4",
    "bunyan-sentry-stream": "^1.2.1",
    "commander": "^2.15.1",
    "copy": "^0.3.1",
    "fs-extra": "^6.0.0",
    "ioredis": "^3.2.2",
    "joi": "^13.2.0",
    "lodash": "^4.17.10",
    "raven": "^2.6.2",
    "sharp": "^0.20.8",
    "source-map-support": "^0.5.6",
    "uuid": "^3.2.1",
    "xdl": "51.3.0"
  }
}
